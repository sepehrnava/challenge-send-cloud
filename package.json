{
  "name": "challenge-send-cloud",
  "description": "Range Calculator Vite Project",
  "author": "Sepehr Nava",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 4174",
    "build-preview": "vite build && vite preview",
    "cy:open": "cypress open",
    "test": "start-server-and-test build-preview http-get://localhost:4173 cy:open",
    "cy:run": "npx cypress run --browser chrome --reporter mochawesome",
    "merge-reports": "npx mochawesome-merge cypress/report/mochawesome-report/*.json > cypress/report/unit-E2E-reports.json",
    "convert-report": "npx marge cypress/report/unit-E2E-reports.json --reportDir ./public/ --inline",
    "full-test-cycle": "start-server-and-test preview http-get://localhost:4174 run-tests",
    "clean-cypress": "rimraf cypress/report cypress/screenshots",
    "run-report": "npm run clean-cypress && npm run cy:run && npm run merge-reports && npm run convert-report",
    "test-report": "start-server-and-test build-preview http-get://localhost:4173 run-report",
    "lint": "eslint lint",
    "fix": "eslint --fix src"
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^5.3.2",
    "axe-core": "^4.9.0",
    "cypress": "^13.8.0",
    "cypress-axe": "^1.5.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.1.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.0.0",
    "mocha": "^10.4.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "sass": "^1.75.0",
    "start-server-and-test": "^2.0.3",
    "vite": "^5.2.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-sass": "^0.1.0"
  },
  "dependencies": {}
}
